{
  "name": "@ant-design/pro-components-editor",
  "version": "0.0.1",
  "description": "pro component editor",
  "keywords": [
    "ant",
    "ant-design",
    "ant-design-pro",
    "antd",
    "design",
    "layout",
    "menu",
    "pro"
  ],
  "homepage": "https://github.com/ant-design/ant-design-pro-layout#readme",
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-pro-layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/ant-design-pro-layout"
  },
  "license": "MIT",
  "author": "qixian.cs@outlook.com",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "build": "father-build && webpack",
    "---------": "",
    "publish": "npm publish",
    "publish:next": "npm publish --tag next",
    "---------    ": "",
    "site:preview": "PREVIEW=1 dumi build",
    "site:build": "dumi build",
    "site:publish": "dumi build && gh-pages -d ./dist",
    "--------- ": "",
    "test": "umi-test",
    "test:update": "UPDATE=1 umi-test --update-snapshot",
    "test:coverage": "umi-test --coverage",
    "---------   ": "",
    "clean": "rm -rf es lib dist build coverage e2e/dist src/.umi* .eslintcache",
    "lint": "npm run lint-eslint && npm run lint:style",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty  && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepublishOnly": "npm run test && npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "prettier": "prettier -c --write \"**/**\"",
    "tsc": "tsc -p tsconfig-check.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-table": "^2.9.7",
    "antd": "^4.2.0",
    "classnames": "^2.2.6",
    "mockjs": "^1.1.0",
    "react-color": "^2.18.1",
