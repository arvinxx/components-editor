    "prettier": "^2.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-github-btn": "^1.1.1",
    "react-markdown": "^4.3.0",
    "slash2": "^2.0.0",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "typescript": "^3.9.5",
    "umi": "^3.2.16",
    "umi-plugin-githubpages": "^2.0.0",
    "umi-test": "^1.9.6",
    "umi-types": "^0.5.11",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "antd": "^4.x",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
