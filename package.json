    "react-intl": "^5.7.0",
    "react-json-view": "^1.19.1",
    "react-resizable": "^1.10.1",
    "react-router-dom": "5.1.2",
    "react-syntax-highlighter": "^12.2.1",
    "stook": "^0.3.0",
    "unstated-next": "^1.1.0",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "stook-devtools": "^0.3.0",
    "@ant-design/compatible": "^1.0.1",
